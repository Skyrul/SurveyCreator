<div class="modal fade" id="modallogicjump" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg " role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">
                    Logic Jump Selector
                </h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">
                        &times;

                    </span>
                </button>
            </div>
            <div class="modal-body" style="height:70vh">
                <div id="logic_jump_">
                    <!--start of template logic jump-->
                    <!-- start of "for which response" row -->
                    <div class="form-group m-form__group row" id="">
                        <label class="col-form-label col-lg-5 col-sm-12">
                            On answer <span id="choice_1">yes</span>:
                        </label>
                        <div class="col-lg-7 col-md-9 col-sm-12">
                            <select class="form-control m-input" name="option" id="select_choice_1">
                                <option value="" disabled>
                                    Select question you want to jump to...
                                </option>
                            </select>
                            @*<span class="m-form__help">
                                    Please select an option.
                                </span>*@
                        </div>
                    </div>

                    <!-- end of "for which response" row -->
                    <!--start "jump to question" row-->
                    <div class="form-group m-form__group row" id="">
                        <label class="col-form-label col-lg-5 col-sm-12">
                            On answer <span id="choice_2">No</span> jump to:
</label>
                        <div class="col-lg-7 col-md-9 col-sm-12">
                            <select class="form-control m-input" name="option" id="select_choice_2">
                                <option value="" disabled>
                                    Select question you want to jump to...
                                </option>
                            </select>
                            @*<span class="m-form__help">
                                    Please select an option.
                                </span>*@
                        </div>
                    </div>
                    <!--end of "jump to question" row-->

                </div>

                <div class="modal-footer">
                   
                    <a href="#" type="button" class="btn btn-success " data-dismiss="modal" aria-label="Close" id="ok_logicJump_button">
                        @*<span aria-hidden="true">*@
                           OK

                        @*</span>*@
                    </a>
                </div>

            </div>
        </div>

    </div>
</div>