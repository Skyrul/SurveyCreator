


<div class="m-portlet__body clonable" id="date_publish_" style="display:none;">
    <div class="form-group m-form__group row">
        <div class="col-md-12 col-lg-12 column sortable ">
            <div style="display:none;"><input id="is_required_" value="false" class="clonable" /></div>
            <div class="container_overlay clonable" id="date_container_overlay_question_picture_" style="display:none;">
                <img id="date_img_place_" name="date_img_place_" src="" alt="" class="image clonable" style="" />
            </div>


            <div id="date_preview_question_" style="font-size: large;color: #000; padding: 10px;" class="clonable">Date type question</div>

            @{ Html.RenderPartial("_QuestionHelp", ViewData["type"] = "date_"); }

            <div class="row">
                <div class="form-group m-form__group row">
                    <div class="col-lg-12 col-md-9 col-sm-12 ">
                        <div class="input-group date clonable" id="m_datepicker_3_">
                            <input id="datepicker" type="text" class="form-control m-input" readonly="" value="05/20/2017">
                            <span class="input-group-addon">
                                <i class="la la-calendar"></i>
                            </span>
                        </div>
                    </div>

                </div>

            </div>



        </div>
    </div>





</div>
