

<div id="agree_publish_" style="display:none;" class="clonable">

    <div class="col-md-12 column sortable ">
        <div style="display:none;"><input id="is_required_" value="false" class="clonable" /></div>
        <div class="container_overlay clonable" id="agree_container_overlay_question_picture_" style="display:none;">
            <img id="agree_img_place_" name="agree_img_place_" src="" alt="" class="image clonable" style="" />
        </div>


        <div id="agree_preview_question_" style="font-size: large;color: #000; padding: 10px;" class="clonable">Agree/Disagree question</div>
        @{ Html.RenderPartial("_QuestionHelp", ViewData["type"] = "agree_"); }

        <div class="row">
            <div class="form-group m-form__group row">
                <div class="col-lg-12 col-md-9 col-sm-12 ">
                    <div class="input-group date clonable" id="agree_buttons_" style="width:165%;">
                        <div id="divBtnIAgreePlacement" class="col-lg-3">
                            <button id="I_Agree_button_" type="button" class="btn btn-lg btn-success clonable">
                                <span><i id="confirmer_I_Agree_button_" class="fa fa-square-o clonable" style="font-size:26px;"></i>&nbsp;&nbsp;&nbsp;I Agree</span>
                            </button>
                        </div>
                        <div id="divBtnIDisagreePlacement" class="col-lg-3">
                            <button id="I_Disagree_button_" type="button" class="btn btn-lg btn-danger clonable">
                                <span><i id="confirmer_I_Disagree_button_" class="fa fa-square-o clonable" style="font-size:26px;"></i>&nbsp;&nbsp;&nbsp;I Disagree</span>
                            </button>

                        </div>                    
                    </div>
                </div>

            </div>

        </div>

        

    </div>

</div>

